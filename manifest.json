{
    "name": "Plug.dj Autow00t",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Autowoots periodically at plug.dj in random times",
    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "icons": {
        "128": "images/icon.png"
    },
    "content_scripts": [ {
        "js": [ "js/lib/jquery-2.1.1.min.js"],
        "matches": [ "http://*/*", "https://*/*"]
    }],
    "browser_action": {
        "default_title": "AutoW00t",
        "default_icon": "images/icon.png",
        "default_popup": "html/popup.html"
    }
}